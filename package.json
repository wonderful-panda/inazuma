{
  "name": "inazuma",
  "version": "0.0.1",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "prebuild:style": "sass src/renderer/style/application.scss dist/renderer/application.css --load-path .",
    "prebuild:copy": "node build/copyFiles.js",
    "prebuild:octicons": "node build/prepare-octicons.js",
    "prebuild:mdi": "node build/prepare-mdi.js",
    "prebuild": "run-s prebuild:*",
    "build:renderer": "webpack --config ./webpack.config.renderer.js",
    "build:browser": "tsc -p src/browser/tsconfig.json",
    "build": "run-s build:renderer build:browser",
    "watch:renderer": "webpack --config ./webpack.config.renderer.js -w",
    "watch:browser": "tsc -p src/browser/tsconfig.json -w",
    "watch": "run-p watch:*",
    "clean": "rimraf dist out node_modules/.cache",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "package": "electron-builder",
    "dev": "run-s prebuild watch",
    "all": "run-s clean format lint build package",
    "start": "electron ."
  },
  "dependencies": {
    "chardet": "^1.3.0",
    "command-line-args": "^5.1.1",
    "electron-devtools-installer": "^3.1.1",
    "fs-extra": "^9.0.1",
    "iconv-lite": "^0.6.2",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.20",
    "node-pty": "^0.9.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/css": "^11.0.0",
    "@mdi/js": "^5.9.55",
    "@primer/octicons": "^9.6.0",
    "@types/chardet": "^0.8.0",
    "@types/command-line-args": "^5.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^9.0.4",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.12.2",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vue/babel-preset-app": "^4.5.9",
    "@vue/composition-api": "^1.0.0-beta.19",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-vue-jsx-modifier": "^1.1.2",
    "babel-plugin-vue-tsx-functional": "^0.1.2",
    "babel-preset-vca-jsx": "^0.3.6",
    "chokidar": "3.4.3",
    "cpx": "^1.5.0",
    "css-loader": "^5.0.1",
    "dayjs": "^1.9.6",
    "electron": "^8.5.5",
    "electron-builder": "^22.9.1",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.1.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "sass": "^1.29.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "upath": "^2.0.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-css-definition": "^0.1.0",
    "vue-material": "1.0.0-beta-15",
    "vue-property-decorator": "^8.3.0",
    "vue-resizesensor": "^0.3.0",
    "vue-template-compiler": "^2.6.11",
    "vue-tsx-support": "^3.1.0",
    "vue-vtable": "1.0.0-beta.8a",
    "vuex": "^3.1.2",
    "vuex-smart-module": "^0.4.6",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "xterm": "^4.3.0",
    "xterm-addon-fit": "^0.4.0"
  }
}
