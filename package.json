    "vue-resizesensor": "^0.3.0",
    "vue-strict-prop": "^0.3.4",
    "vue-template-compiler": "^2.6.10",
    "vue-tsx-support": "^2.3.0",
    "vue-typed-component": "^0.11.0",
    "vue-vtable": "^0.14.1",
    "vuex": "^3.1.1",
    "vuex-smart-module": "https://github.com/wonderful-panda/vuex-smart-module#0.2.7-mod3",
    "webpack": "^4.31.0",
