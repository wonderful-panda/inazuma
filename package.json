{
  "name": "inazuma",
  "version": "0.0.1",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "prebuild:style": "node-sass src/renderer/style/application.scss dist/renderer/application.css",
    "prebuild:copy": "node build/copyFiles.js",
    "prebuild": "run-s prebuild:*",
    "build:renderer": "webpack --config src/renderer/webpack.config.js",
    "build:browser": "tsc -p src/browser/tsconfig.json",
    "build": "run-s build:renderer build:browser",
    "watch:renderer": "webpack --config src/renderer/webpack.config.js -w",
    "watch:browser": "tsc -p src/browser/tsconfig.json -w",
    "watch": "run-p watch:*",
    "clean": "rimraf dist out src/renderer/.cache-loader src/renderer/**/__generated",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "postinstall": "rimraf src/renderer/.cache-loader",
    "package": "electron-builder",
    "dev": "run-s prebuild watch",
    "all": "run-s clean format lint build package",
    "start": "electron ."
  },
  "dependencies": {
    "chardet": "^0.7.0",
    "command-line-args": "^5.1.1",
    "electron-devtools-installer": "^2.2.4",
    "fs-extra": "^8.0.0",
    "iconv-lite": "^0.4.24",
    "ipc-promise": "^0.1.4",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.13",
    "node-pty": "^0.8.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@types/chardet": "^0.5.0",
    "@types/command-line-args": "^5.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^5.1.0",
    "@types/lodash": "^4.14.126",
    "@types/rimraf": "^2.0.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "babel-plugin-vue-jsx-modifier": "^1.0.0",
    "cache-loader": "^3.0.1",
    "chokidar": "2.1.5",
    "cpx": "^1.5.0",
    "css-loader": "^2.1.1",
    "electron": "4.2.2",
    "electron-builder": "^20.40.2",
    "emotion": "^9.2.12",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "^1.3.2",
    "glob": "^7.1.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "monaco-editor": "^0.15.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "node-sass": "4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "typescript-eslint-parser": "^22.0.0",
