{
  "name": "inazuma",
  "version": "0.3.1",
  "type": "module",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf --glob node_modules/.cache dist/* src/react/generated/* src-tauri/generated/* src-tauri/target",
    "prebuild:iconbundle": "ts-node build/create-iconbundle.ts",
    "prebuild:gitinfo": "ts-node build/generate-gitinfo.ts",
    "generate-dts:bindings": "ts-node build/generate-bindings.ts",
    "generate-dts:invoke": "ts-node build/generate-invoke.ts",
    "generate-dts": "run-s generate-dts:*",
    "generate-licenses": "ts-node build/generate-licenses.ts",
    "format:frontend": "biome format --write src/react",
    "format:tauri": "cargo fmt --manifest-path src-tauri/app/Cargo.toml --all",
    "format": "run-s format:frontend format:tauri",
    "lint": "biome lint src/react",
    "check:ts": "tsc -p src/react/tsconfig.json --noEmit",
    "check:tauri": "cargo check --manifest-path src-tauri/app/Cargo.toml",
    "build:ts": "env-cmd -f build/build.env vite build",
    "build:frontend": "run-s prebuild:* generate-dts check:ts build:ts",
    "build:tauri": "tauri build",
    "build:backend": "run-s generate-licenses build:tauri",
    "test:tauri": "cargo test --manifest-path src-tauri/app/Cargo.toml",
    "build": "run-s build:frontend build:backend",
    "watch:ts": "vite",
    "watch:frontend": "run-p \"check:ts -- -w\" watch:ts",
    "watch:tauri": "tauri dev",
    "all": "run-s format lint clean build"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "buffer": "^6.0.3",
    "chardet": "^2.0.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "iconv-lite": "^0.6.2",
    "jotai": "^2.15.1",
    "lodash": "^4.17.20",
    "monaco-editor": "^0.54.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "string_decoder": "^1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@emotion/react": "^11.14.0",
    "@iconify/json": "^2.2.301",
    "@iconify/react": "^5.2.0",
    "@iconify/utils": "^2.2.1",
    "@mui/material": "^7.3.4",
    "@tailwindcss/cli": "^4.1.16",
    "@tailwindcss/vite": "^4.1.16",
    "@tauri-apps/cli": "^2.9.2",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.1.0",
    "env-cmd": "^10.1.0",
    "handlebars": "^4.7.7",
    "jotai-devtools": "^0.13.0",
    "license-checker": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.16",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  }
}
