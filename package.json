{
  "name": "inazuma",
  "version": "0.0.1",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "prebuild:copy": "node build/copyFiles.js",
    "prebuild:octicons": "node build/prepare-octicons.js",
    "prebuild:mdi": "node build/prepare-mdi.js",
    "prebuild": "run-s prebuild:*",
    "build:neon": "electron-build-env neon build inazuma-rust-backend --release",
    "build:renderer": "webpack --config ./webpack.config.renderer.js",
    "build:browser": "tsc -p src/browser/tsconfig.json",
    "build": "run-s build:neon build:renderer build:browser",
    "watch:renderer": "webpack --config ./webpack.config.renderer.js -w",
    "watch:browser": "tsc -p src/browser/tsconfig.json -w",
    "watch": "run-p watch:*",
    "clean": "rimraf dist out node_modules/.cache",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "package": "electron-builder",
    "dev": "run-s prebuild watch",
    "tailwind": "tailwindcss -i ./src/react/index.css -o ./src/react/tailwind.css",
    "watch:tailwind": "tailwindcss -i ./src/react/index.css -o ./src/react/tailwind.css -w",
    "react:typecheck": "tsc -p src/react/tsconfig.json --noEmit",
    "watch:react:typecheck": "tsc -p src/react/tsconfig.json --noEmit -w",
    "vite": "vite",
    "react:serve": "run-p watch:tailwind watch:react:typecheck vite",
    "react:dev": "run-s prebuild build:neon build:browser react:serve",
    "all": "run-s clean format lint build package",
    "start": "electron ."
  },
  "dependencies": {
    "@iconify/react": "^3.0.1",
    "@material-ui/core": "^4.11.4",
    "@reduxjs/toolkit": "^1.6.2",
    "chardet": "^1.3.0",
    "classnames": "^2.3.1",
    "command-line-args": "^5.1.1",
    "electron-devtools-installer": "^3.1.1",
    "fs-extra": "^9.0.1",
    "iconv-lite": "^0.6.2",
    "inazuma-rust-backend": "link:./src/rust-backend",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.20",
    "neon-cli": "^0.9.1",
    "node-pty": "^0.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-transition-group": "^4.4.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/css": "^11.0.0",
    "@mdi/js": "^5.9.55",
    "@primer/octicons": "^9.6.0",
    "@types/chardet": "^0.8.0",
    "@types/command-line-args": "^5.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^9.0.4",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.19",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/rimraf": "^3.0.0",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "@vue/babel-preset-app": "^4.5.9",
    "@vue/composition-api": "^1.0.0-beta.19",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-vue-jsx-modifier": "^1.1.2",
    "babel-plugin-vue-tsx-functional": "^0.1.2",
    "babel-preset-vca-jsx": "^0.3.6",
    "chokidar": "3.4.3",
    "cpx": "^1.5.0",
    "css-loader": "^5.0.1",
    "dayjs": "^1.9.6",
    "electron": "^15.3.0",
    "electron-build-env": "^0.2.0",
    "electron-builder": "^22.9.1",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
