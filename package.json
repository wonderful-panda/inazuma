{
  "name": "inazuma",
  "version": "0.0.2",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules/.cache dist/* src/react/generated/* src-tauri/target",
    "prebuild:iconbundle": "node build/create-iconbundle.js",
    "generate-dts:schema": "cargo run --manifest-path src-tauri/generate/Cargo.toml -- schema --out src/react/generated/tauri-schema.json",
    "generate-dts:invoke": "cargo run --manifest-path src-tauri/generate/Cargo.toml -- invoke --src src-tauri/src/commands.rs --out src/react/generated/tauri-invoke.json",
    "generate-dts:dts": "ts-node build/generate-dts.ts",
    "generate-dts": "run-s generate-dts:schema generate-dts:invoke generate-dts:dts",
    "format": "prettier --write \"src/react/**/*.{ts,tsx,js}\"",
    "lint": "eslint src/react/**/*.{ts,tsx}",
    "check:ts": "tsc -p src/react/tsconfig.json --noEmit",
    "check:tauri": "cargo check --manifest-path src-tauri/Cargo.toml",
    "build:tailwind": "tailwindcss -i ./src/react/index.css -o ./src/react/generated/tailwind.css",
    "build:ts": "cross-env NODE_OPTIONS=\"--max_old_space_size=3072\" vite build",
    "build:frontend": "run-s prebuild:* generate-dts check:ts build:tailwind build:ts",
    "build:tauri": "tauri build",
    "build": "run-s build:frontend build:tauri",
    "watch:ts": "vite",
    "watch:frontend": "run-p \"build:tailwind -w\" \"check:ts -w\" watch:ts",
    "watch:tauri": "tauri dev",
    "watch": "run-p watch:*",
    "all": "run-s clean format lint build"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-rc.5",
    "buffer": "^6.0.3",
    "chardet": "^1.3.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.2",
    "iconv-lite": "^0.6.2",
    "lodash": "^4.17.20",
    "monaco-editor": "^0.33.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-transition-group": "^4.4.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "string_decoder": "^1.3.0",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@iconify/json": "^1.1.424",
    "@iconify/json-tools": "^1.0.10",
    "@iconify/react": "^3.0.1",
    "@mui/material": "^5.6.4",
    "@mui/styles": "^5.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "@tauri-apps/cli": "^1.0.0-rc.10",
    "@types/chardet": "^0.8.0",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-redux": "^7.1.24",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-standard": "^5.0.0",
    "fibers": "^5.0.1",
    "glob": "^8.0.1",
    "handlebars": "^4.7.7",
    "json-schema-to-typescript": "^10.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "~8.4.13",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.2",
    "upath": "^2.0.1",
    "vite": "2.9.7",
    "vite-react-jsx": "^1.1.2"
  }
}
