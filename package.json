{
  "name": "inazuma",
  "version": "0.0.1",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "prebuild:style": "sass src/renderer/style/application.scss dist/renderer/application.css --load-path .",
    "prebuild:copy": "node build/copyFiles.js",
    "prebuild:octicons": "node build/prepare-octicons.js",
    "prebuild": "run-s prebuild:*",
    "build:renderer": "webpack --config ./webpack.config.renderer.js",
    "build:browser": "tsc -p src/browser/tsconfig.json",
    "build": "run-s build:renderer build:browser",
    "watch:renderer": "webpack --config ./webpack.config.renderer.js -w",
    "watch:browser": "tsc -p src/browser/tsconfig.json -w",
    "watch": "run-p watch:*",
    "clean": "rimraf dist out node_modules/.cache",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "package": "electron-builder",
    "dev": "run-s prebuild watch",
    "all": "run-s clean format lint build package",
    "start": "electron ."
  },
  "dependencies": {
    "chardet": "^0.8.0",
    "command-line-args": "^5.1.1",
    "electron-devtools-installer": "^2.2.4",
    "fs-extra": "^8.0.0",
    "iconv-lite": "^0.5.0",
    "ipc-promise": "^0.1.4",
    "jsonschema": "^1.2.5",
    "lodash": "^4.17.19",
    "node-pty": "^0.9.0",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/runtime": "^7.4.4",
    "@primer/octicons": "^9.6.0",
    "@types/chardet": "^0.8.0",
    "@types/command-line-args": "^5.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^8.0.1",
    "@types/lodash": "^4.14.149",
    "@types/rimraf": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vue/babel-preset-app": "^4.5.6",
    "@vue/composition-api": "^1.0.0-beta.17",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-vue-jsx-modifier": "^1.1.2",
    "babel-plugin-vue-tsx-functional": "^0.1.2",
    "babel-preset-vca-jsx": "^0.3.6",
    "chokidar": "3.3.1",
    "cpx": "^1.5.0",
    "css-loader": "^5.0.0",
    "electron": "6.1.7",
    "electron-builder": "^20.40.2",
    "emotion": "^10.0.27",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.1.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "npm-run-all": "^4.1.5",
