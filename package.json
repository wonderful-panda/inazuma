    "fibers": "^5.0.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "mini-css-extract-plugin": "^1.3.6",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.8",
    "prettier": "^2.2.0",
    "sass": "^1.29.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.2.16",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "upath": "^2.0.1",
    "vite": "^2.3.8",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-css-definition": "^0.1.0",
    "vue-material": "1.0.0-beta-15",
    "vue-property-decorator": "^8.3.0",
    "vue-resizesensor": "^0.3.0",
    "vue-template-compiler": "^2.6.11",
    "vue-tsx-support": "^3.2.0",
    "vue-vtable": "1.0.0-beta.8a",
    "vuex": "^3.1.2",
    "vuex-smart-module": "^0.4.6",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "xterm": "^4.3.0",
    "xterm-addon-fit": "^0.4.0"
  }
}
